<TEMPLATE NOME = 'TEMPLATE'>
  <TABELA NOME='ITENS'>
    <COLUNA TM-F='13' SEPARADOR=' '>
      <TITULO>
        <FORMATAR TIPO='ALINHAR' ALINHAR='ESQUERDA' CMP=' '>
          CODIGO
        </FORMATAR>
      </TITULO>
      <FORMATAR TIPO='ALINHAR' ALINHAR='DIREITA' CMP='0'>
            ${{CODIGO}}
      </FORMATAR>
    </COLUNA>
    <COLUNA TM-P='40%' SEPARADOR=' '>
      <TITULO>
        <FORMATAR TIPO='ALINHAR' ALINHAR='ESQUERDA' CMP=' '>
          DESCRICAO
        </FORMATAR>
      </TITULO>
      <FORMATAR TIPO='ALINHAR' ALINHAR='ESQUERDA' CMP=' ' NL='TM'>
        ${{DESCRICAO_ITEM}}
      </FORMATAR>
    </COLUNA>
    <COLUNA TM-F='5' SEPARADOR=' '>
      <TITULO>
        <FORMATAR TIPO='ALINHAR' ALINHAR='DIREITA' CMP=' '>
          QUANTIDADE
        </FORMATAR>
      </TITULO>
      <FORMATAR TIPO='ALINHAR' ALINHAR='DIREITA' CMP='0'>
        ${{QUANTIDADE}}
      </FORMATAR>
    </COLUNA>
  </TABELA>
</TEMPLATE>